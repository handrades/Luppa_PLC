{
  "dashboard": {
    "id": null,
    "title": "Luppa Redis Monitoring",
    "tags": ["luppa", "redis", "cache"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "templating": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "title": "Redis Memory Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Memory Used (MB)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 200
                }
              ]
            },
            "min": 0,
            "max": 512,
            "unit": "decbytes"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Redis Hit/Miss Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_operations_total{result=\"hit\"}[5m]) / (rate(redis_operations_total{result=\"hit\"}[5m]) + rate(redis_operations_total{result=\"miss\"}[5m])) * 100",
            "legendFormat": "Hit Ratio %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 85
                }
              ]
            },
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Redis Operations Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_operations_total{result=\"hit\"}[5m])",
            "legendFormat": "Cache Hits/sec",
            "refId": "A"
          },
          {
            "expr": "rate(redis_operations_total{result=\"miss\"}[5m])",
            "legendFormat": "Cache Misses/sec",
            "refId": "B"
          },
          {
            "expr": "rate(redis_operations_total{result=\"success\"}[5m])",
            "legendFormat": "Operations/sec",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Redis Memory Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Memory Used (MB)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "MB",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Redis Operations by Type",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (operation) (rate(redis_operations_total[5m]))",
            "format": "table",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ],
    "version": 1,
    "uid": "luppa-redis-monitoring",
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "schemaVersion": 27
  }
}
